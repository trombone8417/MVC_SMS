@model DatabaseAccess.ProgrameTable

@{
    ViewBag.Title = "Details";
}
<div>
    <h4>科系</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            學系
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
            開始日期
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StartDate)
        </dd>

        <dt>
            啟用
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IsActive)
        </dd>

        <dt>
            登入人員
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserTable.FullName)
        </dd>


    </dl>
</div>
<p>
    @Html.ActionLink("編輯", "Edit", new { id = Model.ProgrameID }, new { @class = "btn btn-primary" })
    @Html.ActionLink("回主頁", "Index", null, new { @class = "btn btn-default" })
</p>
